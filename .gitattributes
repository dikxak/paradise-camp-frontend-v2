* text=auto

# Explicitly set line endings for specific file types (LF in this case).
*.ts text eol=lf
*.tsx text eol=lf
*.js text eol=lf
*.jsx text eol=lf
*.css text eol=lf
*.html text eol=lf
*.json text eol=lf
*.md text eol=lf

# Binary files should be left untouched.
*.png binary
*.jpg binary
*.gif binary
*.pdf binary
